{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2017", 
        "module": "ESNext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "types": ["types-base", "@gdi/types", "@types/jest"],
        "jsx": "react-jsx"
    }
}
